{
    "plugins": [
        "expandable-chapters",
        "donate",
        "tbfed-pagefooter",
        "anchor-navigation-ex",
        "github-buttons"
    ],
    "pluginsConfig": {
        "donate": {
            "wechat": "https://ws3.sinaimg.cn/large/006tKfTcgy1g1gom432f5j308w08wt93.jpg",
            "alipay": "https://ws3.sinaimg.cn/large/006tKfTcgy1g1goltfohrj308w08wgm4.jpg",
            "title": "你要请我喝奶茶吗？",
            "button": "打赏",
            "alipayText": "支付宝",
            "wechatText": "微信"
        },
        "github-buttons": {
            "buttons": [
                {
                    "user": "tangxiangmin",
                    "repo": "interview",
                    "type": "star",
                    "size": "small"
                },
                {
                    "user": "tangxiangmin",
                    "repo": "interview",
                    "type": "watch",
                    "size": "small"
                }
            ]
        },
        "tbfed-pagefooter": {
            "copyright": "&copy shymean",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "anchor-navigation-ex": {
            "showLevel": false,
            "associatedWithSummary": true,
            "mode": "float",
            "float": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        }
    }
}